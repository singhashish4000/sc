<%= render 'welcome/nav' %>
<div class="container" style='margin-top:50px'>
<div class="row">
<div class="col-xs-12 results" >
 <h3>List of All Volenters</h3>
 <div class="container">
<div class="col-md-10">
      <table class="table table-hover table-striped">
        <thead>
          <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Gender</th>
            <th>Address</th>
            <th>Phone no</th>
            <th>Adhaar no</th>
            <th>Center</th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
          <% @volenters.each do |volenter| %>
            <tr>
              <td><%= volenter.name %></td>
              <td><%= volenter.age %></td>
              <td><%= volenter.gender %></td>
              <td><%= volenter.address %></td>
              <td><%= volenter.phone_no %></td>
              <td><%= volenter.adhaar_no %></td>
              <td><%= volenter.center %></td>
              <td><%= link_to 'Show', volenter %></td>
              <td><%= link_to 'Edit', edit_volenter_path(volenter) %></td>
              <td><%= link_to 'Destroy', volenter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
          <% end %>
        </tbody>
      </table>

      <br>

      <%= link_to 'New Volenter', new_volenter_path %>
    </div>
  </div>  